  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SummerMapper">
	<insert id="insertBoard">
		INSERT INTO SUMMER_TEST_TBL 
		VALUES(SUMMER_SEQ.NEXTVAL,#{boardTitle},#{boardContents})
	</insert>
	
</mapper>